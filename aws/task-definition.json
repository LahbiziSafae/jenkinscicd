{
    "family": "frontend",
    "containerDefinitions": [
        {
            "image": "110609031244.dkr.ecr.eu-west-3.amazonaws.com/workorder",
            "name": "sample-app",
            "essential": true,
            "cpu": 100,
            "memoryReservation": 300,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 443,
                    "hostPort": 443,
                    "protocol": "tcp"
                }
            ]
        }
    ]
}
